<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="leftSideBackground" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Color="#888888" Offset="0"/>
        <GradientStop Color="#000000" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="rightSideBackground" StartPoint="0.5 0" EndPoint="0.5 1">
        <GradientStop Color="#888888" Offset="0"/>
        <GradientStop Color="#666666" Offset="0.25"/>
        <GradientStop Color="#000000" Offset="0.3"/>
        <GradientStop Color="#444444" Offset="0.35"/>
        <GradientStop Color="#333333" Offset="0.55"/>
        <GradientStop Color="#DDDDDD" Offset="0.6"/>
        <GradientStop Color="#333333" Offset="0.65"/>
        <GradientStop Color="#000000" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="playlistBackground" StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Color="#000000" Offset="0"/>
        <GradientStop Color="#888888" Offset="1"/>
    </LinearGradientBrush>
</ResourceDictionary>